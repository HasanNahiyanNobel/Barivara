<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:background="@color/defaultBackground"
	tools:context=".LandlordHouseAddress">

	<AutoCompleteTextView
		android:id="@+id/autoCompleteTextView1"
		android:layout_width="0dp"
		android:layout_height="wrap_content"
		android:layout_marginStart="16dp"
		android:layout_marginLeft="16dp"
		android:layout_marginTop="24dp"
		android:layout_marginEnd="16dp"
		android:layout_marginRight="16dp"
		android:hint="@string/jela"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintHorizontal_bias="0.0"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toBottomOf="@+id/textView2" />

	<ImageView
		android:id="@+id/imageView1"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginBottom="8dp"
		android:clickable="true"
		android:onClick="showDropdownOfDistricts"
		app:layout_constraintBottom_toBottomOf="@+id/autoCompleteTextView1"
		app:layout_constraintEnd_toEndOf="@+id/autoCompleteTextView1"
		app:srcCompat="@drawable/ic_baseline_arrow_dropdown_1" />

	<AutoCompleteTextView
		android:id="@+id/autoCompleteTextView2"
		android:layout_width="0dp"
		android:layout_height="wrap_content"
		android:layout_marginStart="16dp"
		android:layout_marginLeft="16dp"
		android:layout_marginTop="16dp"
		android:layout_marginEnd="16dp"
		android:layout_marginRight="16dp"
		android:hint="@string/upazila"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintHorizontal_bias="0.0"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toBottomOf="@+id/autoCompleteTextView1" />

	<ImageView
		android:id="@+id/imageView2"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginBottom="8dp"
		android:clickable="true"
		android:onClick="showDropdownOfUpazilas"
		app:layout_constraintBottom_toBottomOf="@+id/autoCompleteTextView2"
		app:layout_constraintEnd_toEndOf="@+id/autoCompleteTextView2"
		app:srcCompat="@drawable/ic_baseline_arrow_dropdown_1" />

	<AutoCompleteTextView
		android:id="@+id/autoCompleteTextView3"
		android:layout_width="0dp"
		android:layout_height="wrap_content"
		android:layout_marginStart="16dp"
		android:layout_marginLeft="16dp"
		android:layout_marginTop="16dp"
		android:layout_marginEnd="16dp"
		android:layout_marginRight="16dp"
		android:hint="@string/elaka"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintHorizontal_bias="0.0"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toBottomOf="@+id/autoCompleteTextView2" />

	<ImageView
		android:id="@+id/imageView3"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginBottom="8dp"
		android:clickable="true"
		android:onClick="showDropdownOfAreas"
		app:layout_constraintBottom_toBottomOf="@+id/autoCompleteTextView3"
		app:layout_constraintEnd_toEndOf="@+id/autoCompleteTextView3"
		app:srcCompat="@drawable/ic_baseline_arrow_dropdown_1" />

	<TextView
		android:id="@+id/textView2"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginStart="16dp"
		android:layout_marginLeft="16dp"
		android:layout_marginTop="16dp"
		android:layout_marginEnd="16dp"
		android:layout_marginRight="16dp"
		android:text="@string/basharThikana"
		android:textAppearance="@style/TextAppearance.AppCompat.Large"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>